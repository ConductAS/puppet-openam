com.sun.identity.agents.config.agenturi.prefix=<%= @protocol %>://<%= @host %>:<%= @port %>/amagent
com.sun.identity.agents.config.auth.connection.timeout=<%= @timeout %>
<% if @notification_url %>com.sun.identity.agents.config.change.notification.enable=true
com.sun.identity.client.notification.url=<%= @notification_url %><% end %>
com.sun.identity.agents.config.cookie.name=<%= scope.lookupvar('openam::cookie_name') %>
com.sun.identity.agents.config.cookie.secure=<%= scope.lookupvar('openam::cookie_secure') %>
com.sun.identity.agents.config.debug.level=<%= @debuglevel %>
com.sun.identity.agents.config.fqdn.default=<%= @host %>
com.sun.identity.agents.config.locale=<%= scope.lookupvar('openam::locale') %>
<% @login_urls.each_with_index do |url,index| %>com.sun.identity.agents.config.login.url[<%= index %>]=<%= url %><% end %>
<% @logout_urls.each_with_index do |url,index| %>com.sun.identity.agents.config.logout.url[<%= index %>]=<%= url %><% end %>
<% @agent_logout_urls.each_with_index do |url,index| %>com.sun.identity.agents.config.agent.logout.url[<%= index %>]=<%= url %><% end %>
<% @notenforced_ips.each_with_index do |ip,index| %>com.sun.identity.agents.config.notenforced.ip[<%= index %>]=<%= ip %><% end %>
<% @notenforced_urls.each_with_index do |url,index| %>com.sun.identity.agents.config.notenforced.url[<%= index %>]=<%= url %><% end %>
<% @reset_cookies.each_with_index do |cookie,index| %>com.sun.identity.agents.config.logout.cookie.reset[<%= index %>]=<%= cookie %><% end %>
com.sun.identity.agents.config.repository.location=centralized
com.sun.identity.agents.config.sso.only=<%= ssoonly %>
userpassword=<%= @password_hashed %>
sunIdentityServerDeviceKeyValue=<%= @protocol %>://<%= @host %>:<%= @port %>/
<% @properties.each do |key,value| %><%= @key %>=<%= @value %><% end %>
